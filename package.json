{
  "name": "host",
  "version": "0.1.2",
  "homepage": "https://parvathiajay.github.io/React_JWT/",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:development": "env-cmd -f  .env.development .env && react-scripts build",
    "build:production": "env-cmd -f  .env.production .env && react-scripts build",
    "build:stage": "env-cmd -f  .env.stage .env && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy:development": "copy .env.development .env && npm run build",
    "predeploy:production": "copy .env.production .env && npm run build",
    "predeploy:stage": "copy .env.staging .env && npm run build",
    "clean": "rd /s /q build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^5.0.0"
  }
}